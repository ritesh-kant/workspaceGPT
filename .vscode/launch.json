    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "confluence",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/apps/confluence-extractor",
            "program": "${workspaceFolder}/apps/confluence-extractor/src/index.js",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "cleanup",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/apps/confluence-extractor/src/index.js",
            "envFile": "${workspaceFolder}/apps/confluence-extractor/.env"
        }
    ]
}